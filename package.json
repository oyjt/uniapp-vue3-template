{
  "name": "uniapp-vue3-project",
  "version": "1.0.4",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev:h5": "uni",
    "dev:h5-pro": "uni --mode production",
    "dev:mp-weixin": "uni -p mp-weixin",
    "dev:mp-weixin-prod": "uni -p mp-weixin --mode production",
    "build:h5": "uni build",
    "build:h5-prod": "uni build  --mode production",
    "build:mp-weixin": "uni build -p mp-weixin",
    "build:mp-weixin-prod": "uni build -p mp-weixin --mode production",
    "type-check": "vue-tsc --noEmit",
    "eslint": "eslint --fix",
    "stylelint": "stylelint \"src/**/*.(vue|scss|css)\" --fix",
    "cz": "git add . && npx czg",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {
    "@dcloudio/uni-app": "3.0.0-4000720240327002",
    "@dcloudio/uni-app-plus": "3.0.0-4000720240327002",
    "@dcloudio/uni-components": "3.0.0-4000720240327002",
    "@dcloudio/uni-h5": "3.0.0-4000720240327002",
    "@dcloudio/uni-mp-alipay": "3.0.0-4000720240327002",
    "@dcloudio/uni-mp-baidu": "3.0.0-4000720240327002",
    "@dcloudio/uni-mp-jd": "3.0.0-4000720240327002",
    "@dcloudio/uni-mp-kuaishou": "3.0.0-4000720240327002",
    "@dcloudio/uni-mp-lark": "3.0.0-4000720240327002",
    "@dcloudio/uni-mp-qq": "3.0.0-4000720240327002",
    "@dcloudio/uni-mp-toutiao": "3.0.0-4000720240327002",
    "@dcloudio/uni-mp-weixin": "3.0.0-4000720240327002",
    "@dcloudio/uni-mp-xhs": "3.0.0-4000720240327002",
    "@dcloudio/uni-quickapp-webview": "3.0.0-4000720240327002",
    "pinia": "2.0.36",
    "uview-plus": "^3.1.49",
    "vue": "3.3.11",
    "vue-i18n": "^9.1.9",
    "z-paging": "^2.7.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.11.6",
    "@dcloudio/types": "^3.4.8",
    "@dcloudio/uni-automator": "3.0.0-4000720240327002",
    "@dcloudio/uni-cli-shared": "3.0.0-4000720240327002",
    "@dcloudio/uni-stacktracey": "3.0.0-4000720240327002",
    "@dcloudio/vite-plugin-uni": "3.0.0-4000720240327002",
    "@types/node": "^20.12.2",
    "@typescript-eslint/parser": "^7.4.0",
    "@uni-helper/uni-app-types": "^0.5.9",
    "@unocss/eslint-plugin": "^0.58.8",
    "@vue/runtime-core": "^3.3.11",
    "@vue/tsconfig": "^0.5.1",
    "czg": "^1.9.1",
    "eslint": "^8.57.0",
    "lint-staged": "^15.2.2",
    "miniprogram-api-typings": "^3.12.2",
    "picocolors": "^1.0.0",
    "postcss-html": "^1.6.0",
    "postcss-scss": "^4.0.9",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.72.0",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.3.1",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.4.3",
    "unocss": "^0.58.8",
    "unocss-applet": "^0.7.7",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^4.5.3",
    "vite-plugin-clean-build": "^1.1.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-replace-image-url": "^1.1.1",
    "vue-tsc": "^2.0.7"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{scss,less,style,html}": "stylelint --fix",
    "*.vue": [
      "eslint --fix",
      "stylelint --fix"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  }
}
